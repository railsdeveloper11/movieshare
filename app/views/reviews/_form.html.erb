<%= form_for @review do |f| %>
  <%= f.fields_for :movie do |a|%>
    <%= a.label :movie_url %>
    <%= a.text_field :movie_url %>
  <% end %>
  <%= f.label :description %>:
  <%= f.text_field :description %><br />

  <%= f.label :rating %>:
  <%= f.number_field :rating, min: 1, max: 5%><br />

  <%= f.submit %>
<% end %>
